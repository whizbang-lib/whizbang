{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Aspire AppHost",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}/ECommerce.AppHost/ECommerce.AppHost.csproj"
    },
    {
      "name": "Launch Aspire AppHost (coreclr)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "setup and build AppHost",
      "program": "${workspaceFolder}/ECommerce.AppHost/bin/Debug/net10.0/ECommerce.AppHost.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ECommerce.AppHost",
      "stopAtEntry": false,
      "console": "internalConsole",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development",
        "DOTNET_DASHBOARD_OTLP_ENDPOINT_URL": "https://localhost:21030",
        "DOTNET_RESOURCE_SERVICE_ENDPOINT_URL": "https://localhost:22246",
        "ASPNETCORE_URLS": "https://localhost:17286;http://localhost:15195",
        "ASPIRE_ALLOW_UNSECURED_TRANSPORT": "true"
      },
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}",
        "osx": {
          "command": "open"
        }
      }
    }
  ]
}
